@{
  Layout = "_Layout";
}
<h1>Order Details:</h1>

<p>Vendor: @Model["vendor"].Name</p>
<p>Title: @Model["order"].Title</p>
<p>Details: @Model["order"].Details</p>
<p>Date: @Model["order"].Date</p>
<p>Price: @Model["order"].Price</p>

<a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order</a><br>
<a href='/vendors/@Model["vendor"].Id/'>Back to this vendors' orders</a><br>
<a href='/vendors'>View all vendors</a>